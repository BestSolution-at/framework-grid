<?xml version="1.0" encoding="ASCII"?>
<grid:MGrid
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:grid="http://www.bestsolution.at/framework/grid/1.0"
    xsi:schemaLocation="http://www.bestsolution.at/framework/grid/1.0 ../../../../../../../../at.bestsolution.framework.grid.model/model/grid.ecore"
    defaultConfiguration="//@configurationSets.0">
  <columns id="firstname"
      titleKey="header.firstname"
      minWidth="100"
      maxWidth="100">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="firstname"/>
    </cellValueFunction>
    <autoFilterConfiguration
        xsi:type="grid:MFreeTextAutoFilterConfiguration"/>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="firstname"/>
  </columns>
  <columns id="lastname"
      titleKey="header.lastname"
      minWidth="100">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="lastname"/>
    </cellValueFunction>
    <autoFilterConfiguration
        xsi:type="grid:MFreeTextAutoFilterConfiguration"/>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="lastname"/>
  </columns>
  <columns id="street"
      titleKey="header.street"
      minWidth="20"
      maxWidth="200"
      autoWidth="true">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="address"/>
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="street"/>
    </cellValueFunction>
    <autoFilterConfiguration
        xsi:type="grid:MFreeTextAutoFilterConfiguration"/>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="street"/>
  </columns>
  <columns id="number"
      titleKey="header.number"
      minWidth="100"
      maxWidth="100"
      alignment="RIGHT">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="address"/>
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="number"/>
    </cellValueFunction>
    <cellTextFunction
        xsi:type="grid:MFormattedCellTextFunction">
      <pattern
          xsi:type="grid:MStringPattern"
          pattern="000.00"/>
    </cellTextFunction>
    <autoFilterConfiguration
        xsi:type="grid:MFreeTextAutoFilterConfiguration"/>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="number"/>
  </columns>
  <columns id="birthdate"
      titleKey="header.birthdate"
      minWidth="100"
      maxWidth="150"
      alignment="CENTER"
      sortingBehavior="UP_DOWN_DEFAULT">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="birthdate"/>
    </cellValueFunction>
    <cellTextFunction
        xsi:type="grid:MFormattedCellTextFunction"
        formatType="DATE">
      <pattern
          xsi:type="grid:MStringPattern"
          pattern="dd. MM. yyyy"/>
    </cellTextFunction>
    <autoFilterConfiguration
        xsi:type="grid:MComboAutoFilterConfiguration">
      <predefinedValues
          xsi:type="grid:MDefaultAutoFilterEntry"
          resourceKey="filter.all"/>
      <predefinedValues
          xsi:type="grid:MDefaultAutoFilterEntry"
          resourceKey="filter.empty"
          type="EMPTY"/>
      <predefinedValues
          xsi:type="grid:MDefaultAutoFilterEntry"
          resourceKey="filter.notempty"
          type="NOT_EMPTY"/>
      <cellTextFunction
          xsi:type="grid:MFormattedCellTextFunction"
          formatType="DATE">
        <pattern
            xsi:type="grid:MStringPattern"
            pattern="ddMMyyyy"/>
      </cellTextFunction>
    </autoFilterConfiguration>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="birthdate"/>
  </columns>
  <columns id="localizedBirthdate"
      titleKey="header.birthdate"
      minWidth="100"
      maxWidth="150"
      alignment="CENTER"
      sortingBehavior="UP_DOWN_DEFAULT">
    <cellValueFunction
        xsi:type="grid:MPathCellValueFunction">
      <segments
          xsi:type="grid:MSimplePathSegment"
          featureName="birthdate"/>
    </cellValueFunction>
    <cellTextFunction
        xsi:type="grid:MFormattedCellTextFunction"
        formatType="DATE">
      <pattern
          xsi:type="grid:MReferencePattern"
          patternKey="datepattern"/>
    </cellTextFunction>
    <metaDataList
        xsi:type="grid:MSimpleMetaData"
        topic="persongrid/cellinfo"
        metaDataValue="birthdate2"/>
  </columns>
  <configurations>
    <columns
        column="//@columns.0"/>
    <columns
        column="//@columns.1"/>
    <columns
        column="//@columns.2"/>
    <columns
        column="//@columns.3"/>
    <columns
        column="//@columns.4"/>
    <columns
        column="//@columns.5"/>
  </configurations>
  <configurationSets
      viewConfiguration="//@configurations.0"
      defaultSortColumn="//@columns.0"/>
  <resources
      locale="de">
    <entries
        key="header.firstname"
        value="Vorname"/>
    <entries
        key="header.lastname"
        value="Nachname"/>
    <entries
        key="header.street"
        value="Stra&#xdf;e"/>
    <entries
        key="header.number"
        value="Nummer"/>
    <entries
        key="header.birthdate"
        value="Geburtsdatum"/>
    <entries
        key="datepattern"
        value="dd. MM. yyyy"/>
    <entries
        key="filter.all"
        value="Alle"/>
    <entries
        key="filter.empty"
        value="Leere"/>
    <entries
        key="filter.notempty"
        value="Nicht leere"/>
    <entries
        key="filter.custom"
        value="Benutzerdefiniert"/>
  </resources>
  <resources
      locale="en">
    <entries
        key="header.firstname"
        value="First name"/>
    <entries
        key="header.lastname"
        value="Last name"/>
    <entries
        key="header.street"
        value="Street"/>
    <entries
        key="header.number"
        value="Number"/>
    <entries
        key="header.birthdate"
        value="Birthdate"/>
    <entries
        key="datepattern"
        value="MMM dd, yyyy"/>
    <entries
        key="filter.all"
        value="All"/>
    <entries
        key="filter.empty"
        value="Empty"/>
    <entries
        key="filter.notempty"
        value="Not empty"/>
    <entries
        key="filter.custom"
        value="Custom"/>
  </resources>
</grid:MGrid>
